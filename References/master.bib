@article{Hallenbeck2004,
abstract = {This chapter reviews various types of proposed biohydrogen processes, including dark fermentations, along with the potentially limiting factors. Several mechanisms by which photosynthesis could yield hydrogen (H{\textless}inf{\textgreater}2{\textless}/inf{\textgreater})'such as direct biophotolysis, indirect biophotolysis, and photofermentations'are also examined in the chapter. The basis for any biological H{\textless}inf{\textgreater}2{\textless}/inf{\textgreater}-producing system is an enzyme that reduces protons to hydrogen. Currently, the three known enzymes that carry out this reaction are nitrogenase, Fe-hydrogenase, and NiFe hydrogenase. The catalytic activity of these enzymes varies enormously, and it is in theory possible that at least in some cases the quantity or activity of the H{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} evolving enzyme could limit the overall process. In theory, photosynthetic efficiencies could be as high as 10{\%} (conversion of total light energy into product). However, under full sunlight conditions, up to 90{\%} of the photons captured by the photosynthetic apparatus are not used in productive photosynthesis but rather decay as heat or fluorescence. Improving photosynthetic efficiencies is essential to the realization of practical microalgal processes for H{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} production. Fermentative processes present an interesting yet largely unexplored avenue for the biological production of H{\textless}inf{\textgreater}2{\textless}/inf{\textgreater}. One fermentation of interest would be to harness the endogenous metabolic machinery of algal cells to quantitatively convert stored carbohydrate (starch or glycogen in green algae and cyanobacteria, respectively) to H{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} in a strictly dark reaction. In this case, the critical factor is the amount of H{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} that can be produced per mole of substrate.},
annote = {From Duplicate 1 (Fundamentals and limiting processes of biological hydrogen production - Hallenbeck, P. C.)

NULL},
author = {Hallenbeck, P. C.},
doi = {10.1016/B978-008044356-0/50008-7},
file = {:home/opencafe/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Hallenbeck - 2004 - Fundamentals and limiting processes of biological hydrogen production(2).pdf:pdf},
