issn = {0271-2091},
journal = {Int. J. Numer. Meth. Fluids},
keywords = {1455 de,a posterori error estimation,cfd laboratory,concordia university,correspondence to,department of mechanical engineering,edge-based error estimator,mesh,mesh adaptation,solver coupling,spring analogy,structured and unstructured grids},
